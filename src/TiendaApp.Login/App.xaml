<Application x:Class="TiendaApp.Login.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TiendaApp.Login">
    <Application.Resources>
        <!-- Elimina el ResourceDictionary anidado y pon directamente MergedDictionaries -->
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/TiendaApp.Resources;component/AppThemes.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/TiendaApp.Resources;component/styles/DictionaryStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <!-- Puedes agregar recursos locales aquÃ­ si los necesitas -->
        </ResourceDictionary>
    </Application.Resources>
</Application>
